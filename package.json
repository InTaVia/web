{
	"name": "@intavia/web",
	"version": "0.2.0",
	"license": "MIT",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=18.14.0",
		"npm": ">=8.19.0"
	},
	"packageManager": "npm@9.3.1",
	"scripts": {
		"analyze-bundle": "cross-env BUNDLE_ANALYZER='enabled' next build --no-lint",
		"build": "next build",
		"dev": "next dev",
		"format:check": "prettier . --cache --check --ignore-path ./.gitignore",
		"format:fix": "npm run format:check -- --write",
		"generate:favicons": "dotenv -- tsx ./scripts/generate-favicons.ts",
		"lint:check": "run-p lint:*:check",
		"lint:fix": "run-p lint:*:fix",
		"lint:code:check": "eslint . --cache --cache-location .next/cache/eslint/ --ext cjs,mjs,ts,tsx --ignore-path ./.gitignore",
		"lint:code:fix": "npm run lint:code:check -- --fix",
		"lint:styles:check": "stylelint \"./**/*.css\" --allow-empty-input --cache --cache-location .next/cache/stylelint/ --ignore-path ./.gitignore",
		"lint:styles:fix": "npm run lint:styles:check -- --fix",
		"prebuild": "npm run generate:favicons",
		"prepare": "run-p setup generate:*",
		"setup": "simple-git-hooks || exit 0",
		"start": "next start",
		"test": "exit 0",
		"types:check": "tsc --noEmit",
		"validate": "run-s format:check lint:check types:check test"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.12",
		"@heroicons/react": "^2.0.16",
		"@intavia/api-client": "^0.1.16",
		"@intavia/data-import": "^0.1.3",
		"@intavia/ui": "^0.1.15",
		"@mapbox/mapbox-gl-draw": "^1.4.1",
		"@next/bundle-analyzer": "^13.2.3",
		"@popperjs/core": "^2.11.6",
		"@reduxjs/toolkit": "^1.9.3",
		"@stefanprobst/assert": "^1.0.3",
		"@stefanprobst/group-by": "^1.1.0",
		"@stefanprobst/key-by": "^1.0.1",
		"@stefanprobst/log": "^1.0.3",
		"@stefanprobst/next-error-boundary": "^3.0.2",
		"@stefanprobst/next-i18n": "^1.2.0",
		"@stefanprobst/next-page-metadata": "^2.0.5",
		"@stefanprobst/pagination": "^1.0.0",
		"@stefanprobst/request": "^0.1.11",
		"allotment": "^1.18.1",
		"d3": "^7.0.0",
		"d3-beeswarm": "^0.0.5",
		"date-fns": "^2.29.3",
		"final-form": "^4.20.9",
		"final-form-arrays": "^3.1.0",
		"final-form-focus": "^1.1.2",
		"html-to-image": "^1.11.11",
		"lodash.get": "^4.4.2",
		"lucide-react": "^0.121.0",
		"mapbox-gl": "npm:empty-npm-package@^1.0.0",
		"maplibre-gl": "^2.4.0",
		"next": "^13.2.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-final-form": "^6.5.9",
		"react-final-form-arrays": "^3.1.4",
		"react-grid-layout": "^1.3.4",
		"react-map-gl": "^7.0.21",
		"react-popper": "^2.3.0",
		"react-redux": "^8.0.5",
		"react-resize-detector": "^8.0.4",
		"redux-persist": "^6.0.0",
		"sharp": "^0.30.7",
		"zod": "^3.20.6"
	},
	"devDependencies": {
		"@next/eslint-plugin-next": "^13.2.3",
		"@stefanprobst/eslint-config": "^1.0.10",
		"@stefanprobst/eslint-config-next": "^7.0.6",
		"@stefanprobst/eslint-config-react": "^1.0.4",
		"@stefanprobst/favicons": "^2.2.2",
		"@stefanprobst/prettier-config": "^3.0.0",
		"@stefanprobst/stylelint-config": "^3.0.1",
		"@stefanprobst/tsconfig": "^2.2.2",
		"@types/d3": "^7.4.0",
		"@types/final-form-focus": "^1.1.3",
		"@types/lodash.get": "^4.4.7",
		"@types/mapbox__mapbox-gl-draw": "^1.3.3",
		"@types/node": "^18.14.5",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@types/react-grid-layout": "^1.3.2",
		"@types/react-redux": "^7.1.25",
		"@types/sharp": "^0.30.5",
		"autoprefixer": "^10.4.13",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.0.0",
		"eslint": "^8.35.0",
		"eslint-plugin-tailwindcss": "^3.10.1",
		"lint-staged": "^13.1.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.21",
		"postcss-custom-media": "^9.1.2",
		"postcss-nesting": "^11.2.1",
		"prettier": "^2.8.4",
		"simple-git-hooks": "^2.8.1",
		"stylelint": "^15.2.0",
		"tailwindcss": "^3.2.7",
		"tsx": "^3.12.3",
		"typescript": "^4.9.5"
	},
	"browserslist": {
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		],
		"production": [
			">=0.75%",
			"not dead",
			"not op_mini all"
		]
	},
	"eslintConfig": {
		"extends": [
			"@stefanprobst/eslint-config",
			"@stefanprobst/eslint-config-react",
			"@stefanprobst/eslint-config-next",
			"plugin:tailwindcss/recommended"
		],
		"settings": {
			"tailwindcss": {
				"config": "./tailwind.config.cjs"
			}
		}
	},
	"lint-staged": {
		"*.@(cjs|mjs|ts|tsx)": [
			"eslint --cache --cache-location .next/cache/eslint/ --fix",
			"prettier --cache --write"
		],
		"*.css": [
			"stylelint --cache --cache-location .next/cache/stylelint/ --fix",
			"prettier --cache --write"
		],
		"*.!(cjs|css|mjs|ts|tsx)": "prettier --cache --ignore-unknown --write"
	},
	"postcss": {
		"plugins": {
			"postcss-custom-media": {},
			"tailwindcss/nesting": "postcss-nesting",
			"tailwindcss": {},
			"autoprefixer": {}
		}
	},
	"prettier": "@stefanprobst/prettier-config",
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged",
		"pre-push": "npm run validate"
	},
	"stylelint": {
		"extends": "@stefanprobst/stylelint-config"
	}
}
